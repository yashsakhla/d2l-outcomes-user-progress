{
  "name": "d2l-outcomes-user-progress",
  "version": "1.6.0",
  "description": "Web components for Outcomes User Progress page",
  "scripts": {
    "license-check": "d2l-license-checker -p",
    "lint": "npm run lint:eslint && npm run lint:lit && npm run lint:style",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer {src,test}/**/*.js --strict",
    "lint:style": "stylelint **/*.js",
    "lint:lang": "mfv -e",
    "format": "npm run format:lang && npm run format:eslint && npm run format:style",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:lang": "mfv remove-extraneous && mfv add-missing",
    "format:style": "npm run lint:style -- --fix",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --dedupe --watch --open --preserve-symlinks",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "karma start",
    "test:headless:watch": "karma start --auto-watch=true --single-run=false"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "dependencies": {
    "@brightspace-ui/core": "^1.147.1",
    "@open-wc/dedupe-mixin": "^1",
    "@polymer/iron-a11y-announcer": "^3",
    "@polymer/iron-collapse": "^3",
    "d2l-outcomes-overall-achievement": "github:Brightspace/d2l-outcomes-overall-achievement#semver:^1",
    "lit-element": "^2",
    "siren-sdk": "github:BrightspaceHypermediaComponents/siren-sdk#semver:^1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7",
    "@brightspace-ui/stylelint-config": "^0.2",
    "@open-wc/testing": "^2",
    "@open-wc/testing-karma": "^4",
    "@web/dev-server": "^0.1",
    "@webcomponents/webcomponentsjs": "^2",
    "d2l-license-checker": "^4",
    "deepmerge": "^4",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.14",
    "eslint-plugin-chai-friendly": "^0.7",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-mocha": "^9",
    "eslint-plugin-sort-class-members": "^1",
    "lit-analyzer": "^1",
    "messageformat-validator": "github:bearfriend/messageformat-validator",
    "mocha": "^9",
    "stylelint": "^13"
  }
}
