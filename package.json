{
  "name": "d2l-outcomes-user-progress",
  "version": "1.5.0",
  "description": "Web components for Outcomes User Progress page",
  "scripts": {
    "lang:build": "lang-build -es \"\t\" -c langtools/config.json",
    "lang:copy": "lang-copy -svc langtools/config.json",
    "license-check": "d2l-license-checker",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer src/**/*.js --strict",
    "format": "npm run format:eslint",
    "format:eslint": "npm run lint:eslint -- --fix",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --dedupe --watch --open --preserve-symlinks"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "dependencies": {
    "@brightspace-ui/core": "^1.30.1",
    "@polymer/iron-a11y-announcer": "^3.0.2",
    "@polymer/iron-collapse": "^3.0.1",
    "d2l-outcomes-overall-achievement": "github:Brightspace/d2l-outcomes-overall-achievement#semver:^1",
    "lit-element": "^2.4.0",
    "siren-sdk": "BrightspaceHypermediaComponents/siren-sdk#semver:^1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7",
    "@webcomponents/webcomponentsjs": "^2",
    "d2l-license-checker": "^3",
    "es-dev-server": "^2",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.14",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "lit-analyzer": "^1"
  }
}
